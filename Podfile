# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'CopyBoard' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  inhibit_all_warnings!
  
  # Pods for CopyBoard
  pod 'RxSwift'
  pod 'RxCocoa'
  
  pod 'RealmSwift'
  # https://github.com/SnapKit/SnapKit
  pod 'SnapKit', '~> 5.0.0'
  # https://github.com/malcommac/SwiftDate
  pod 'SwiftDate'
  # https://github.com/Ramotion/circle-menu
  pod 'CircleMenu'
  pod 'Bugly'
  
  pod 'WhatsNewKit'

  target 'CopyBoardTests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'Keyboard' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  inhibit_all_warnings!
  # Pods for Keyboard
  
  pod 'RealmSwift'
  pod 'SnapKit', '~> 5.0.0'
  
end

target 'StickerShare' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  inhibit_all_warnings!
  # Pods for StickerShare
  pod 'RealmSwift'
  pod 'SnapKit', '~> 5.0.0'

end
